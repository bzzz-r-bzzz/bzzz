{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "worker",
  "main": "./src/index.tsx",
  "compatibility_date": "2025-06-17",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,

  "ai": {
    "binding": "AI"
  },

  "env": {
    "development": {
      "ai": {
        "binding": "AI"
      }
    },
    "production": {
      "ai": {
        "binding": "AI"
      },
      "assets": {
        "directory": "./dist/client",
        "binding": "ASSETS"
      }
    }
  }
}
